return {
  {
    "hedyhli/outline.nvim",
    cmd = { "Outline", "OutlineOpen" },
    keys = {
      { "<leader>o", "<cmd>Outline<CR>", desc = "Outline Toggle" },
    },
    opts = {
      outline_window = {
        position = "left", -- "right"에서 "left"로 변경하여 Explorer와 같은 열에 배치
        width = 40, -- Explorer의 폭과 동일하게 35로 통일
        auto_close = false,
      },
    },
  },
}
